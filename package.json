{
  "name": "railway-logbook",
  "version": "0.0.0",
  "description": "A map application for logging rail journeys",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepareData": "sh ./osmium-scripts/prepare.sh",
    "populateDb": "node --max-old-space-size=8192 ./node_modules/tsx/dist/cli.mjs src/scripts/populateDb.ts",
    "exportRouteData": "tsx src/scripts/exportRoutes.ts",
    "importRouteData": "tsx src/scripts/importRoutes.ts",
    "uploadMapData": "pscp ./data/*.geojson railmap@railmap.zlatkovsky.cz:/home/railmap/osm-trains/data/",
    "downloadMapData": "pscp railmap@railmap.zlatkovsky.cz:/home/railmap/osm-trains/data/*.geojson ./data/",
    "downloadRouteData": "pscp railmap@railmap.zlatkovsky.cz:/home/railmap/osm-trains/data/*.sql ./data/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timichal/railway-logbook.git"
  },
  "author": "Michal Zlatkovsk√Ω",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timichal/railway-logbook/issues"
  },
  "homepage": "https://github.com/timichal/railway-logbook#readme",
  "dependencies": {
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "jose": "^6.1.0",
    "maplibre-gl": "^5.10.0",
    "next": "^16.0.1",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "tailwindcss": "^4.1.16"
  }
}
