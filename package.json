{
  "name": "railway-logbook",
  "version": "0.0.0",
  "description": "A map application for logging rail journeys",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepareMapData": "sh ./osmium-scripts/prepare.sh",
    "importMapData": "node --max-old-space-size=8192 ./node_modules/tsx/dist/cli.mjs src/scripts/importMapData.ts",
    "verifyRouteData": "tsx src/scripts/verifyRouteData.ts",
    "exportRouteData": "tsx src/scripts/exportRoutes.ts",
    "importRouteData": "tsx src/scripts/importRoutes.ts",
    "applyVectorTiles": "tsx src/scripts/applyVectorTiles.ts",
    "markAllRoutesInvalid": "tsx src/scripts/markAllRoutesInvalid.ts",
    "listStations": "tsx src/scripts/listStations.ts",
    "addTripsTable": "tsx src/scripts/addTripsTable.ts",
    "downloadMapData": "pscp railmap@railmap.zlatkovsky.cz:/home/railmap/osm-trains/data/*.geojson ./data/",
    "downloadRouteData": "pscp railmap@railmap.zlatkovsky.cz:/home/railmap/osm-trains/data/*.sql ./data/",
    "deployMapData": "sh ./osmium-scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timichal/railway-logbook.git"
  },
  "author": "Michal Zlatkovsk√Ω",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timichal/railway-logbook/issues"
  },
  "homepage": "https://github.com/timichal/railway-logbook#readme",
  "dependencies": {
    "@rapideditor/country-coder": "^5.6.1",
    "autoprefixer": "^10.4.24",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.3.1",
    "jose": "^6.1.3",
    "maplibre-gl": "^5.18.0",
    "next": "^16.1.6",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "transliteration": "^2.6.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/transliteration": "^1.6.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "tailwindcss": "^4.2.0"
  }
}
